{
  "name": "create-root-schema",
  "version": "1.0.0-2",
  "main": "dist/index.js",
  "module": "src/index.js",
  "repository": "https://github.com/gsandf/create-root-schema",
  "author": "GS&F Devs <mindreactor@gsandf.com> (https://gsandf.com/)",
  "license": "MIT",
  "files": [
    "src/index.js",
    "dist/index.js"
  ],
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": true
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-export-extensions"
    ]
  },
  "eslintConfig": {
    "extends": "gsandf",
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-gsandf": "^1.0.0",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.0",
    "lodash.merge": "^4.6.1"
  },
  "scripts": {
    "build": "webpack --mode production --target node --output-library create-root-schema --output-library-target umd -o ./dist/index.js",
    "lint": "eslint src",
    "prepublish": "yarn build",
    "test": "ava"
  }
}
